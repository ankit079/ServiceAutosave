<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    <link href="~/node_modules/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
    <link href="~/node_modules/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
    <script defer src="~/node_modules/bootstrap/dist/fonts/fontawesome-all.js"></script>

</head>
<body>
    <div>
        @RenderBody()
    </div>
    <div>
        <footer>
            @RenderSection("footer", required: false)
            @*@await Component.InvokeAsync("Greeter")*@
        </footer>
    </div>
    <environment>
        <script src="~/node_modules/jquery/dist/jquery.js"></script>
        <script src="~/node_modules/jquery-validation/dist/jquery.validate.js"></script>
        <script src="~/node_modules/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>
    </environment>
</body>
</html>
